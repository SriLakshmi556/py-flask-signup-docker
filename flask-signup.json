{
    "family": "flask-signup",
    "networkMode": "awsvpc",
    "containerDefinitions": [
        {
            "image": "sreelakshmi555/flask-signup:v_%BUILD_NUMBER%",
            "name": "flask-signup",
            "cpu": 256,
            "memory": 512,
            "essential": true,
            "portMappings": [
                {
                    "containerPort": 5000,
                    "hostPort": 5000
                }
            ]
        }
    ],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "256",
    "memory": "512"
}
